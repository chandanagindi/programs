import java.util.*;
public class Lcm
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=0,d=0;
        for(int i=1;i<=a;i++)
        {
            if(a%i==0&&b%i==0)
            c=i;
            d=(a*b)/c;
        }
        System.out.printf("LCM of %d and %d is %d",a,b,d);
    }
}

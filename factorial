import java.util.*;
 public class Factorial
{
    public static void main(String[] args)
    {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      if(a==6)
      {
          System.out.println("Yes");
      }
      else
      {
          System.out.println("no"); 
      }
    }
}

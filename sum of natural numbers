import java.util.*;
public class Sumofnum
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int p=sc.nextInt();
        int b=0;
        for(int i=a;i<=p;i++)
        {
            b=b+i;
            if(i==p)
            {
                System.out.println(b);
            }
        }
    }
}

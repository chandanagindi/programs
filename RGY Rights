import java.util.*;
public class Lights
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        float a=sc.nextFloat();
        float b=sc.nextFloat();
        float c=sc.nextFloat();
        float d=a/b;
        if(d>c)
        {
            System.out.println("yes");
        }
         else
        {
            System.out.println("no"); 
        }
    }
}

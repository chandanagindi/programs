import java.util.Scanner;
public class Series4
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
         double b=0.5;
        System.out.printf(b + " ");
        for(int i=0;i<a;i++)
        {
            b=b*3;
            System.out.printf(b + " ");
        }
    }
}

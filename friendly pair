import java.util.*;
public class Friendly
{
    public static void main(String[] args)
    {
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int c=a/6;
       int d=b/28;
       if(c==d)
       {
           System.out.println("Friendly pair");
       }
       else 
       {
           System.out.println("Not friendly pair");
       }
    }
}

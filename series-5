import java.util.*;
public class Series5
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        System.out.print(11*11+" ");
        int s=11;
        for(int i=0;i<=a;i++)
        {
            s=s+4;
            System.out.print(s*s+" ");
        }
    }
}

import java.util.*;
public class Series10
{
    public static void main(String[] args)
    {
         Scanner sc=new Scanner(System.in);
         int n=sc.nextInt();
         double a=95.0;
         double b=20.5;
         System.out.printf("\t"+a);
         for(int i=1;i<n;i++)
         {
             a=a+b;
             b=b+2;
             System.out.printf("\t"+a);
         }
    }
}
